<h1>New Warehouse</h1>

<%= error_messages_for :warehouse %>

<%= form_for(:warehouse, :url => admin_warehouses_path) do |f| %>
  <p>
    <b>Warehouse Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Comment</b><br />
    <%= f.text_field :comment %>
  </p>

  <p>
    <label>
      <%= f.check_box :tracks_inventory %>
      Tracks Inventory
    </label>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', admin_warehouses_path %>
