<h1>Editing Warehouse</h1>

<%= error_messages_for :warehouse %>

<%= form_for(:warehouse, :url => admin_warehouse_path(@warehouse), :html => { :method => :patch }) do |f| %>
  <p>
    <b>Warehouse Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Comment</b><br />
    <%= f.text_field :comment %>
  </p>

  <p>
    <%= submit_tag "Update" %>
  </p>
<% end %>

<%= link_to 'Back', admin_warehouses_path %>
