<h1>Editing region</h1>

<%= error_messages_for :region %>

<%= form_for(:region, :url => admin_region_path(@region), :html => { :method => :patch }) do |f| %>
  <%= render(partial: 'form', locals: { f: f }) %>
  <p><button class="debounced">Update</button></p>
<% end %>

<%= link_to 'Show', admin_region_path(@region) %> |
<%= link_to 'Back', admin_regions_path %>
