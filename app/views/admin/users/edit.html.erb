<h1>Editing User</h1>

<%= error_messages_for :user %>

<%= form_for(:user, :url => admin_user_path(@user), :html => { :method => :patch }) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>

  <p>
    <button class="debounced">Update</button>
  </p>
<% end %>

<%= link_to 'Show', admin_user_path(@user) %> |
<%= link_to 'Back', admin_users_path %>
